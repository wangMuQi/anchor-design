{
  "name": "anchor-design",
  "version": "0.22.17",
  "description": "A Component Library For Vue",
  "scripts": {
    "start": "npm run watch",
    "watch": "webpack-dev-server --hot --inline --config build/webpack.dev.conf.js --progress --colors",
    "build": "npm run clean && webpack -p --config build/webpack.prod.conf.js --progress --colors",
    "clean": "rm -rf lib && rm -rf static",
    "pub:major": "npm run prepub && npm version minor && npm publish",
    "pub:minor": "npm run prepub && npm version minor && npm publish",
    "pub": "npm version patch && npm publish",
    "prepub": "npm run build"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/51idc/anchor-design.git"
  },
  "keywords": [
    "anchor",
    "component",
    "vue",
    "ui"
  ],
  "bugs": {
    "url": "https://github.com/51idc/anchor-design/issues"
  },
  "homepage": "https://github.com/51idc/anchor-design#readme",
  "peerDependencies": {
    "vue": "^2.3.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue": "^2.3.4",
    "vue-loader": "^12.2.1",
    "vue-router": "^2.6.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.4",
    "vuex": "^2.3.1",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {},
  "author": "liuzmeng liuzhimengcn@gmail.com",
